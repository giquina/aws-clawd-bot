    "research": {},
    "vercel": {},
    "reminders": {
      "maxReminders": 50,
      "maxDelayHours": 24
    },
    "pomodoro": {
      "defaultDuration": 25,
      "maxDuration": 180,
      "minDuration": 1
    },
    "moltbook": {},
    "browser": {
      "headless": true,
      "timeout": 30000
    },
    "deadlines": {},
    "companies": {},
    "governance": {},
    "intercompany": {},
    "digest": {},
    "overnight": {},
    "receipts": {},
    "invoices": {
      "defaultDueDays": 30,
      "defaultCurrency": "GBP",
      "pdfFormat": "text"
    },
    "image-analysis": {},
    "document-analyzer": {
      "maxFileSizeMB": 50,
      "analysisModel": "opus"
    },
    "coder": {},
    "review": {},
    "stats": {},
    "actions": {},
    "multi-repo": {},
    "project-creator": {},
    "voice": {},
    "voice-call": {
      "defaultVoice": "amy",
      "maxCallDuration": 300
    },
    "video": {},
    "files": {},
    "weather": {
      "cacheTTLMinutes": 30,
      "defaultCity": "London",
      "units": "metric"
    },
    "currency": {
      "cacheTTLMinutes": 60,
      "supportedCurrencies": 150,
      "apiProvider": "exchangerate-api.com"
    },
    "timezone": {
      "defaultCities": ["London", "New York", "Tokyo", "Sydney"],
      "supportedCities": 200
    },
    "news": {
      "cacheTTLMinutes": 60,
      "maxArticles": 5,
      "defaultCountry": "us",
      "categories": ["tech", "business", "science", "sports", "general"]
    },
    "quickmath": {
      "defaultTipPercent": 15,
      "currency": "GBP",
      "decimalPlaces": 2
    },
    "ai-settings": {},
    "autonomous-config": {},
    "chat-management": {
      "allowMultipleHQ": false,
      "defaultNotifications": "all"
    },
    "hq-commands": {
      "cacheTTLMinutes": 30,
      "maxSearchResults": 20
    },
    "project-context": {
      "cacheTTLMinutes": 60
    },
    "remote-exec": {
      "defaultTimeout": 60000,
      "maxOutputLength": 3000
    },
    "audit": {
      "retentionDays": 30,
      "maxQueryResults": 100
    },
    "docker": {
      "defaultTimeout": 30000,
      "maxOutputLength": 2500,
      "maxLogLines": 50
    },
    "backup": {
      "retentionDays": 7,
      "maxBackups": 20
    },
    "monitoring": {
      "cacheTTLSeconds": 30,
      "thresholds": {
        "cpu": { "warning": 70, "critical": 90 },
        "memory": { "warning": 70, "critical": 90 },
        "disk": { "warning": 80, "critical": 95 }
      }
    },
    "secrets": {
      "encryptionAlgorithm": "aes-256-gcm",
      "maskLength": 4,
      "auditRetentionDays": 90
    },
    "analytics": {
      "cacheTTLMinutes": 5,
      "defaultPeriod": "week",
      "maxCacheEntries": 20
    },
    "image-gen": {
      "model": "stability-ai/sdxl",
      "costPerGeneration": 0.02,
      "maxWidth": 1024,
      "maxHeight": 1024,
      "defaultSteps": 25,
      "guidanceScale": 7.5,
      "requiresConfirmation": true
    },
    "meeting": {
      "maxActiveSessions": 1,
      "maxAudioChunks": 50,
      "transcriptionModel": "whisper-large-v3",
      "analysisModel": "opus",
      "storageDir": "meetings"
    }
  }
}
